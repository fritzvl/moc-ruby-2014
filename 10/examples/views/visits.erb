<h1>Visits list for service <%= label %> </h1>
<table class="table table-bordered table-responsive table-striped">
  <thead>
  <tr>
    <th>Data</th>
    <th>Visited at</th>
  </tr>
  </thead>
  <tbody>
  <% visits.each do |visit| %>
      <tr>
        <td><%= visit.data %></td>
        <td><%= visit.created_at %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<%= link_to "Services", "/services" , class: "btn btn-success"%>

<%= paginate visits %>
